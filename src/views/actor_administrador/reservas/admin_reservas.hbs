<header>
        <nav class="Portafolio-Servicios">
            <ul class="contenedor-iconos-header">
                <li class="icon icon1"><a href="/admin_inicio"><span></span></a></li>
                <li class="icon icon2"><a href="/admin_educacion"> <span></span></a></li>
                <li class="icon icon3"><a href="/admin_reservas"> <span></span></a></li>
                <li class="icon icon4"><a href="/admin_contacto"> <span></span></a></li>
                <li class="icon icon5"><a href="/admin_cartama"> <span></span></a></li>
                <li class="icon icon6"><a href="/admin_usuario"> <span></span></a></li>
            </ul>
            <div class="contendor-iconos-nombres">
                <p>Inicio</p>
                <p>Educacion</p>
                <p>Reservas</p>
                <p>Contacto</p>        
                <p>Cartama</p>
                <p>Usuario </p>
            </div> 
        </nav>
    </header>

   <main>
    <h1 class="title_h1 desk">Reservas</h1>

    <section class="Usuarios_Solcitud">
        {{#each query_reservation}} 
        <article class="info_usuario">
            <div class="nombre-fecha">
              <p>Numero de Identificacion:   <output class="nombre-del-usuario">{{id_usuario}}</output> </p>
            </div>
            
            <div class="informacion-solicitud">
                <p>Informacion de la solicitud:</p>
                <output class="Descripcion-solicitud">{{descripcion}}</output>
            </div>
            <div class="informacion-contacto">
                <p>Salon: <output>{{id_ambiente}}</output></p>
                
                <p>Telefono: </p><output class="contacto-Telefono contacto-Telefono_desktop">{{telefono}}</output>
                 
            </div>
            
        </article>
        {{/each}}
        
    </section>

   </main>
